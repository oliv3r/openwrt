/dts-v1/;

#include <dt-bindings/clock/rtl83xx-clk.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	#address-cells = <1>;
	#size-cells = <1>;

	compatible = "realtek,rtl930x-soc";

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;

		led-boot = &sys_led;
		led-failsafe = &sys_led;
		led-running = &sys_led;
		led-upgrade = &sys_led;
	};

	chosen {
		bootargs = "earlycon";
		stdout-path = "serial0:115200n8";
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		frequency = <800000000>;

		cpu@0 {
			compatible = "mips,mips34Kc";
			reg = <0>;
		};
	};

	cpuintc: cpuintc {
		compatible = "mti,cpu-interrupt-controller";
		#address-cells = <0>;
		#interrupt-cells = <1>;
		interrupt-controller;
	};

	keys {
		compatible = "gpio-keys";

		reset_button: reset {
			label = "reset";
			linux,code = <KEY_RESTART>;

			status = "disabled";
		};
	};

	leds {
		compatible = "gpio-leds";

		pinctrl-names = "default";
		pinctrl-0 = <&pinmux_disable_sys_led>;

		sys_led: sys-led {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			gpios = <&gpio0 0 GPIO_ACTIVE_HIGH>;
			panic-indicator;

			status = "disabled";
		};
	};

	lx_clk: lx_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency  = <175000000>;
	};

	lxp: lexra-busp {
		compatible = "lexra-bus", "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges = <0x18000000 0x18000000 0x200000>;

		gpio0: gpio-controller@18003300 {
			compatible = "realtek,rtl9300-gpio", "realtek,otto-gpio";
			reg = <0x18003300 0x1c>, <0x18003338 0x8>;

			gpio-controller;
			#gpio-cells = <2>;
			ngpios = <24>;

			interrupt-controller;
			#interrupt-cells = <2>;
			interrupt-parent = <&intc>;
			interrupts = <13 1>;
		};

		intc: interrupt-controller@18003000 {
			compatible = "realtek,rtl9300-intc", "realtek,rtl-intc";
			reg = <0x18003000 0x18>, <0x18003018 0x18>;
			interrupt-controller;
			#interrupt-cells = <2>;

			interrupt-parent = <&cpuintc>;
			interrupts = <2>, <3>, <4>, <5>, <6>, <7>;
		};

		qspif0: qspif@18001200 {
			compatible = "realtek,rtl8380-spi";
			reg = <0x18001200 0x100>;

			#address-cells = <1>;
			#size-cells = <0>;
		};

		timer0: timer@18003200 {
			compatible = "realtek,rtl930x-timer", "realtek,otto-timer";
			reg = <0x18003200 0x10>,
			      <0x18003210 0x10>,
			      <0x18003220 0x10>,
			      <0x18003230 0x10>,
			      <0x18003240 0x10>;

			interrupt-parent = <&intc>;
			interrupts = <7 4>, <8 4>, <9 4>, <10 4>, <11 4>;
			clocks = <&lx_clk>;
		};

		uart0: uart@18002000 {
			compatible = "ns16550a";
			reg = <0x18002000 0x100>;

			clocks = <&lx_clk>;

			interrupt-parent = <&intc>;
			interrupts = <30 1>;

			reg-io-width = <1>;
			reg-shift = <2>;
			fifo-size = <1>;
			no-loopback-test;
		};

		uart1: uart@18002100 {
			compatible = "ns16550a";
			reg = <0x18002100 0x100>;

			clocks = <&lx_clk>;

			interrupt-parent = <&intc>;
			interrupts = <31 0>;

			reg-io-width = <1>;
			reg-shift = <2>;
			fifo-size = <1>;
			no-loopback-test;

			status = "disabled";
		};

		watchdog0: watchdog@18003260 {
			compatible = "realtek,rtl9300-wdt";
			reg = <0x18003260 0xc>;

			realtek,reset-mode = "soc";

			clocks = <&lx_clk>;
			timeout-sec = <30>;

			interrupt-parent = <&intc>;
			interrupt-names = "phase1", "phase2";
			interrupts = <5 4>, <6 4>;
		};
	};

	lx0: lexra-bus0 {
		compatible = "lexra-bus", "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges = <0x1b000000 0x1b000000 0x10000>;

		ethernet0: ethernet@1b00a300 {
			compatible = "realtek,rtl838x-eth";
			reg = <0x1b00a300 0x100>;

			interrupt-parent = <&intc>;
			interrupts = <24 3>;

			phy-mode = "internal";
			mdio-bus = <&mdio0>;

			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};

		i2c0: i2c0@1b00036c {
			reg = <0x1b00036c 0x1c>;
			compatible = "realtek,rtl9300-i2c", "realtek,rtl930x-i2c";
			#address-cells = <1>;
			#size-cells = <0>;

			status = "disabled";
		};

		i2c1: i2c1@1b000388 {
			reg = <0x1b000388 0x1c>;
			compatible = "realtek,rtl9300-i2c", "realtek,rtl930x-i2c";
			#address-cells = <1>;
			#size-cells = <0>;

			status = "disabled";
		};

		i2cmux0 {
			compatible = "realtek,i2c-mux-rtl9300", "realtek,i2c-mux-rtl930x";
			#address-cells = <1>;
			#size-cells = <0>;
			i2c-parent = <&i2c0>;

			i2c0_0: i2c0@0 {
				reg = <0>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <9>;
				scl-pin = <8>;
				status = "disabled";
			};

			i2c0_1: i2c0@1 {
				reg = <1>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <10>;
				scl-pin = <8>;
				status = "disabled";
			};

			i2c0_2: i2c0@2 {
				reg = <2>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <11>;
				scl-pin = <8>;
				status = "disabled";
			};

			i2c0_3: i2c0@3 {
				reg = <3>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <12>;
				scl-pin = <8>;
				status = "disabled";
			};

			i2c0_4: i2c0@4 {
				reg = <4>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <13>;
				scl-pin = <8>;
				status = "disabled";
			};

			i2c0_5: i2c0@5 {
				reg = <5>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <14>;
				scl-pin = <8>;
				status = "disabled";
			};

			i2c0_6: i2c0@6 {
				reg = <6>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <15>;
				scl-pin = <8>;
				status = "disabled";
			};

			i2c0_7: i2c0@7 {
				reg = <7>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <16>;
				scl-pin = <8>;
				status = "disabled";
			};
		};

		i2cmux1 {
			compatible = "realtek,i2c-mux-rtl9300", "realtek,i2c-mux-rtl930x";
			#address-cells = <1>;
			#size-cells = <0>;
			i2c-parent = <&i2c1>;

			i2c1_0: i2c1@0 {
				reg = <0>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <9>;
				scl-pin = <17>;
				status = "disabled";
			};

			i2c1_1: i2c1@1 {
				reg = <1>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <10>;
				scl-pin = <17>;
				status = "disabled";
			};

			i2c1_2: i2c1@2 {
				reg = <2>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <11>;
				scl-pin = <17>;
				status = "disabled";
			};

			i2c1_3: i2c1@3 {
				reg = <3>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <12>;
				scl-pin = <17>;
				status = "disabled";
			};

			i2c1_4: i2c1@4 {
				reg = <4>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <13>;
				scl-pin = <17>;
				status = "disabled";
			};

			i2c1_5: i2c0@5 {
				reg = <5>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <14>;
				scl-pin = <17>;
				status = "disabled";
			};

			i2c1_6: i2c1@6 {
				reg = <6>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <15>;
				scl-pin = <17>;
				status = "disabled";
			};

			i2c1_7: i2c1@7 {
				reg = <7>;
				#address-cells = <1>;
				#size-cells = <0>;

				sda-pin = <16>;
				scl-pin = <17>;
				status = "disabled";
			};
		};

		mdio0: mdio-bus@1b00ca00 {
			compatible = "realtek,rtl930x-mdio", "realtek,rtl838x-mdio";
			#address-cells = <1>;
			#size-cells = <0>;
		};

		pinmux_led: pinmux@1b00cc00 {
			compatible = "pinctrl-single";
			reg = <0x1b00cc00 0x4>;

			pinctrl-single,bit-per-mux;
			pinctrl-single,register-width = <32>;
			pinctrl-single,function-mask = <0x1>;
			#pinctrl-cells = <2>;

			/* enable GPIO 0 */
			pinmux_disable_sys_led: disable_sys_led {
				pinctrl-single,bits = <0x0 0x0 0x1000>;
			};
		};

		switch@1b000000 {
			compatible = "realtek,rtl83xx-switch";

			interrupt-parent = <&intc>;
			interrupts = <23 2>;

			switch0_ports: ports {
				#address-cells = <1>;
				#size-cells = <0>;

				/* Internal SoC <-> Switch ethernet connection */
				port@28 {
					reg = <28>;
					ethernet = <&ethernet0>;
					phy-mode = "internal";

					fixed-link {
						speed = <1000>;
						full-duplex;
					};
				};
			};
		};
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};
};

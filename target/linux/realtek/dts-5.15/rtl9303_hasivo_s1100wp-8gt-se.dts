&i2c0_0 {
		/* POE controller Port1 - Port4 */
		hs104pti@1a {
			compatible = "hasivo,hs104pti";
			reg = <0x1a>;
		};

		/* POE controller Port5 - Port8 */
		hs104pti@2a {
			compatible = "hasivo,hs104pti";
			reg = <0x2a>;
		};
};

&mdio0 {
	/* 8x RTL8221B PHYs */
	phy0: ethernet-phy@0 {
		reg = <0>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <0 0>;
		sds = <2>;
	};

	phy1: ethernet-phy@1 {
		reg = <1>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <0 1>;
		sds = <3>;
	};

	phy2: ethernet-phy@2 {
		reg = <2>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <0 2>;
		sds = <4>;
	};

	phy3: ethernet-phy@3 {
		reg = <3>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <0 3>;
		sds = <5>;
	};

	phy4: ethernet-phy@4 {
		reg = <4>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <1 0>;
		sds = <6>;
	};

	phy5: ethernet-phy@5 {
		reg = <5>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <1 1>;
		sds = <7>;
	};

	phy6: ethernet-phy@6 {
		reg = <6>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <1 2>;
		sds = <8>;
	};

	phy7: ethernet-phy@27 {
		reg = <7>;
		compatible = "ethernet-phy-ieee802.3-c45";
		rtl9300,smi-address = <1 3>;
		sds = <9>;
	};
};

&switch0_ports {
	port@0 {
		reg = <0>;
		label = "lan1";
		phy-handle = <&phy0>;
		phy-mode = "hsgmii";
		led-num = <(
			RTL93XX_LED_NUM0 | /* Orange */
			RTL93XX_LED_NUM1 | /* Lime */
			RTL93XX_LED_NUM2   /* Lime */
		)>;
		led-set = <0>;
	};

	port@8 {

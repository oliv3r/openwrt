&mdio0 {
	/* External 8 port 10/100/1000Mbit RTL8218D PHY (QSGMII) first half */
	phy0: ethernet-phy@0 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0>;
		rtl9300,smi-address = <0 0>;
		sds = <0>;
		/* Disabled because we do not know how to bring up again */
		/* reset-gpios = <&gpio0  GPIO_ACTIVE_LOW>; */
	};

	phy1: ethernet-phy@1 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <1>;
		rtl9300,smi-address = <0 1>;
	};

	phy2: ethernet-phy@2 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <2>;
		rtl9300,smi-address = <0 2>;
	};

	phy3: ethernet-phy@3 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <3>;
		rtl9300,smi-address = <0 3>;
	};

	/* External 8 port 10/100/1000Mbit RTL8218D PHY (QSGMII) second half */
	phy4: ethernet-phy@4 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <4>;
		rtl9300,smi-address = <0 4>;
		sds = <1>;
	};

	phy5: ethernet-phy@5 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <5>;
		rtl9300,smi-address = <0 5>;
	};

	phy6: ethernet-phy@6 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <6>;
		rtl9300,smi-address = <0 6>;
	};

	phy7: ethernet-phy@7 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <7>;
		rtl9300,smi-address = <0 7>;
	};

	/* External 8 port 10/100/1000Mbit RTL8218D PHY (XGMII) */
	phy8: ethernet-phy@8 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <8>;
		rtl9300,smi-address = <1 8>;
		sds = <2>;
		/* Disabled because we do not know how to bring up again */
		/* reset-gpios = <&gpio0  GPIO_ACTIVE_LOW>; */
	};

	phy9: ethernet-phy@9 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <9>;
		rtl9300,smi-address = <1 9>;
	};

	phy10: ethernet-phy@10 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <10>;
		rtl9300,smi-address = <1 10>;
	};

	phy11: ethernet-phy@11 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <11>;
		rtl9300,smi-address = <1 11>;
	};

	phy12: ethernet-phy@12 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <12>;
		rtl9300,smi-address = <1 12>;
	};

	phy13: ethernet-phy@13 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <13>;
		rtl9300,smi-address = <1 13>;
	};

	phy14: ethernet-phy@14 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <14>;
		rtl9300,smi-address = <1 14>;
	};

	phy15: ethernet-phy@15 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <15>;
		rtl9300,smi-address = <1 15>;
	};

	/* External 8 port 10/100/1000Mbit RTL8218D PHY (XGMII) */
	phy16: ethernet-phy@16 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <16>;
		rtl9300,smi-address = <2 16>;
		sds = <3>;
		/* Disabled because we do not know how to bring up again */
		/* reset-gpios = <&gpio0  GPIO_ACTIVE_LOW>; */
	};

	phy17: ethernet-phy@17 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <17>;
		rtl9300,smi-address = <2 17>;
	};

	phy18: ethernet-phy@18 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <18>;
		rtl9300,smi-address = <2 18>;
	};

	phy19: ethernet-phy@19 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <19>;
		rtl9300,smi-address = <2 19>;
	};

	phy20: ethernet-phy@20 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <20>;
		rtl9300,smi-address = <2 20>;
	};

	phy21: ethernet-phy@21 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <21>;
		rtl9300,smi-address = <2 21>;
	};

	phy22: ethernet-phy@22 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <22>;
		rtl9300,smi-address = <2 22>;
	};

	phy23: ethernet-phy@23 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <23>;
		rtl9300,smi-address = <2 23>;
	};

	/* SFP+ Ports */
	phy24: ethernet-phy@24 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <24>;
		phy-is-integrated;
		sds = <4>;
	};

	phy25: ethernet-phy@25 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <25>;
		phy-is-integrated;
		sds = <6>;
	};

	phy26: ethernet-phy@26 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <26>;
		phy-is-integrated;
		sds = <8>;
	};

	phy27: ethernet-phy@27 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <27>;
		phy-is-integrated;
		sds = <9>;
	};
};

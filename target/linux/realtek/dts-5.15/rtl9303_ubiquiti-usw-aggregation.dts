&uart1 {
	status = "okay";
};

&switch0_ports {
	port@0 {
		reg = <0>;
		led-num = <(
			RTL93XX_LED_NUM0 | /* Unknown Color */
			RTL93XX_LED_NUM1   /* White */
		)>;
		led-set = <0>;
		label = "lan8";
		phy-handle = <&phy0>;
		phy-mode = "10gbase-r";
		managed = "in-band-status";
		sfp = <&sfp7>;
	};


/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/leds/leds-realtek.h>

#include "rtl9303.dtsi"
#include "rtl9303_common_8s.dtsi"

/ {
	compatible = "ubiquiti,usw-aggregation", "realtek,rtl9303-soc", "realtek,rtl930x-soc";
	model = "Ubiquiti USW-Aggregation Switch";

	led_set {
		compatible = "realtek,rtl9303-leds", "realtek,rtl930x-leds";

		led_set0 = /bits/ 16 <
			/* LED0 */
			(
				RTL93XX_LED_SET_LINK_ACTIVITY |
				RTL93XX_LED_SET_LINK_UP_SOLID |
				RTL93XX_LED_SET_1000M_LINK |
				RTL93XX_LED_SET_100M_LINK |
				RTL93XX_LED_SET_10M_LINK
			)
			/* LED1 */
			(
				RTL93XX_LED_SET_LINK_ACTIVITY |
				RTL93XX_LED_SET_LINK_UP_SOLID |
				RTL93XX_LED_SET_10G_LINK
			)
			/* LED2 */
			RTL93XX_LED_SET_NONE
			/* LED3 */
			RTL93XX_LED_SET_NONE
		>;
	};

	memory@0 {
		reg = <0x0 0x10000000>; /* 256 MiB */
		device_type = "memory";
	};
};

&i2c0_4 {
	status = "okay";

	gpioexp0: pca9555@21 {
		reg = <0x21>;
		compatible = "nxp,pca9555";
		gpio-controller;
		#gpio-cells = <2>;
	};

	gpioexp1: pca9555@24 {
		reg = <0x24>;
		compatible = "nxp,pca9555";
		gpio-controller;
		#gpio-cells = <2>;
	};
};

&qspif0 {
	status = "okay";

	flash@0 {
		reg = <0>;
		compatible = "macronix,mx25l12805d", "mxicy,mx25l12805d", "jedec,spi-nor";
		spi-max-frequency = <13300000>;

		partitions: partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0xa0000>;
				read-only;
			};

			partition@a0000 {
				reg = <0xa0000 0x10000>;
				label = "u-boot-env";
			};

			partition@b0000 {
				reg = <0xb0000 0xf50000>;
				compatible = "openwrt,uimage", "denx,uimage";
				label = "firmware";
				openwrt,ih-magic = <0x93001210>;
			};
		};
	};
};

&reset_button {
	gpios = <&gpio0 3 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&sfp0 {
	los-gpio = <&gpioexp1 0 GPIO_ACTIVE_HIGH>;
	tx-fault-gpio = <&gpioexp1 1 GPIO_ACTIVE_HIGH>;
	mod-def0-gpio = <&gpioexp1 2 GPIO_ACTIVE_LOW>;
	tx-disable-gpios = <&gpioexp1 3 GPIO_ACTIVE_LOW>;
};

